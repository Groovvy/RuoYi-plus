<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baosight.mgmt.mapper.CanMapper">

    <resultMap type="Can" id="CanResult">
        <result property="id" column="id"/>
        <result property="canCode" column="can_code"/>
        <result property="canStyle" column="can_style"/>
        <result property="neckline" column="neckline"/>
        <result property="width" column="width"/>
        <result property="height" column="height"/>
        <result property="modelUrl" column="model_url"/>
        <result property="status" column="status"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateBy" column="update_by"/>
    </resultMap>

    <sql id="selectCanVo">
        select id, can_code, can_style, neckline, width, height, model_url, status, create_time, create_by, update_time,
        update_by from t_mgmt_can
    </sql>

    <select id="selectCanList" parameterType="Can" resultMap="CanResult">
        <include refid="selectCanVo"/>
        <where>
            <if test="canCode != null  and canCode != ''">and can_code = #{canCode}</if>
            <if test="canStyle != null  and canStyle != ''">and can_style = #{canStyle}</if>
            <if test="neckline != null ">and neckline = #{neckline}</if>
            <if test="width != null ">and width = #{width}</if>
            <if test="height != null ">and height = #{height}</if>
            <if test="modelUrl != null  and modelUrl != ''">and model_url = #{modelUrl}</if>
            <if test="status != null  and status != ''">and status = #{status}</if>
        </where>
    </select>

    <select id="selectCanById" parameterType="Long" resultMap="CanResult">
        <include refid="selectCanVo"/>
        where id = #{id}
    </select>

    <insert id="insertCan" parameterType="Can">
        insert into t_mgmt_can
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null ">id,</if>
            <if test="canCode != null  and canCode != ''">can_code,</if>
            <if test="canStyle != null  and canStyle != ''">can_style,</if>
            <if test="neckline != null ">neckline,</if>
            <if test="width != null ">width,</if>
            <if test="height != null ">height,</if>
            <if test="modelUrl != null  and modelUrl != ''">model_url,</if>
            <if test="status != null  and status != ''">status,</if>
            <if test="createTime != null ">create_time,</if>
            <if test="createBy != null  and createBy != ''">create_by,</if>
            <if test="updateTime != null ">update_time,</if>
            <if test="updateBy != null  and updateBy != ''">update_by,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null ">#{id},</if>
            <if test="canCode != null  and canCode != ''">#{canCode},</if>
            <if test="canStyle != null  and canStyle != ''">#{canStyle},</if>
            <if test="neckline != null ">#{neckline},</if>
            <if test="width != null ">#{width},</if>
            <if test="height != null ">#{height},</if>
            <if test="modelUrl != null  and modelUrl != ''">#{modelUrl},</if>
            <if test="status != null  and status != ''">#{status},</if>
            <if test="createTime != null ">#{createTime},</if>
            <if test="createBy != null  and createBy != ''">#{createBy},</if>
            <if test="updateTime != null ">#{updateTime},</if>
            <if test="updateBy != null  and updateBy != ''">#{updateBy},</if>
        </trim>
    </insert>

    <update id="updateCan" parameterType="Can">
        update t_mgmt_can
        <trim prefix="SET" suffixOverrides=",">
            <if test="canCode != null  and canCode != ''">can_code = #{canCode},</if>
            <if test="canStyle != null  and canStyle != ''">can_style = #{canStyle},</if>
            <if test="neckline != null ">neckline = #{neckline},</if>
            <if test="width != null ">width = #{width},</if>
            <if test="height != null ">height = #{height},</if>
            <if test="modelUrl != null  and modelUrl != ''">model_url = #{modelUrl},</if>
            <if test="status != null  and status != ''">status = #{status},</if>
            <if test="createTime != null ">create_time = #{createTime},</if>
            <if test="createBy != null  and createBy != ''">create_by = #{createBy},</if>
            <if test="updateTime != null ">update_time = #{updateTime},</if>
            <if test="updateBy != null  and updateBy != ''">update_by = #{updateBy},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCanById" parameterType="Long">
        delete from t_mgmt_can where id = #{id}
    </delete>

    <delete id="deleteCanByIds" parameterType="String">
        delete from t_mgmt_can where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>